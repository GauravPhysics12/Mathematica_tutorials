   RowBox[{
    RowBox[{"g", "[", 
     RowBox[{"x_", ",", "x0_", ",", "v0_"}], "]"}], ":=", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"1", "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{"v0", "^", "2"}], "+", 
         RowBox[{"F", "[", 
          RowBox[{"u", ",", "x0"}], "]"}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"u", ",", "x0", ",", "x"}], "}"}]}], "]"}]}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Step", " ", "4"}], ":", 
     RowBox[{
      RowBox[{"Solve", " ", 
